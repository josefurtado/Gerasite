@model Gerasite.Dominio.Entidades.Templates.Mostruario

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Meu Template</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <link href="~/Content/Templates/Mostruario/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Templates/Mostruario/layoutGeraSite.css" />
    <link rel="stylesheet" href="~/Content/Templates/Mostruario/mostruario.css">
    <link rel="stylesheet" href="~/Content/Templates/Mostruario/mostruarioLaranja.css">
</head>
<body>
    <nav class="navbar navbar-fixed-top navbar-default corNav hidden-lg hidden-sm hidden-md" id="#menuLayout">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#menu">
                    <span class="icon-bar corIconBar"></span>
                    <span class="icon-bar corIconBar"></span>
                    <span class="icon-bar corIconBar"></span>
                </button>
                <span class="navbar-brand item-sidebar">@Html.DisplayFor(model => model.Titulo)</span>
            </div>

            <div id="menu" class="collapse navbar-collapse text-center">
                <ul class="nav navbar-nav corNav ">
                    <li><a href="#Home" class="item-sidebar itemMenu2">Home</a></li>
                    <li><a href="#Gallery" class="item-sidebar itemMenu2">@Html.DisplayFor(model => model.TituloSessao1)</a></li>
                    <li><a href="#Life" class="item-sidebar itemMenu2">@Html.DisplayFor(model => model.TituloSessao2)</a></li>
                    <li><a href="#Living" class="item-sidebar itemMenu2">@Html.DisplayFor(model => model.TituloSessao3)</a></li>
                    <li><a href="#Reflection" class="item-sidebar itemMenu2">@Html.DisplayFor(model => model.TituloSessao4)</a></li>

                </ul>
            </div>
        </div>
    </nav>

    <nav class="container">
        <div class="row">
            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 preenchimento navFixa hidden-xs">
                <ul class="nav nav-sidebar corNav text-left">
                    <li class="item-sidebar tituloMenu">@Html.DisplayFor(model => model.Titulo)</li>
                    <li><a href="#Home" class="item-sidebar itemMenuInicio">Home</a></li>
                    <li><a href="#Gallery" class="item-sidebar itemMenu">@Html.DisplayFor(model => model.TituloSessao1)</a></li>
                    <li><a href="#Life" class="item-sidebar itemMenu">@Html.DisplayFor(model => model.TituloSessao2)</a></li>
                    <li><a href="#Living" class="item-sidebar itemMenu">@Html.DisplayFor(model => model.TituloSessao3)</a></li>
                    <li><a href="#Reflection" class="item-sidebar itemMenu">@Html.DisplayFor(model => model.TituloSessao4)</a></li>
                </ul>
            </div>
        </div>
    </nav>

    <div class="container cont">
        <div class="row">
            <div class="col-lg-3 col-md-3 col-sm-3 "></div>
            <div class="col-lg-9 col-md-9 col-sm-9"> </div>
        </div>
        <div class="row cont2 hidden-lg hidden-sm hidden-md">
            <div class="col-xs-12"> <br></div>
        </div>
    </div>

    <header id="Home" class="container cont2">
        <div class="row">
            <div class="col-lg-3 col-md-3 col-sm-3 "></div>
            <div class="col-lg-9 col-md-9 col-sm-9 corNav2">
                <span class="titulo corTexto">@Html.DisplayFor(model => model.Titulo)</span>
            </div>
        </div>
    </header>

    <section id="Gallery" class="container cont2">
        <div class="row">
            <div class="col-lg-3 col-md-3 col-sm-3 "></div>
            <div class="col-lg-9 col-md-9 col-sm-9  corNav2">
                <span class="tituloSessao  corTitulo">@Html.DisplayFor(model => model.TituloSessao1).</span>

                <div class="row  container-fluid">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12"> <br><br></div>

                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                        <img class="img-responsive thumbnail" src="@Url.Content(Model.Img1)" alt="Image" />
                    </div>

                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                        <img class="img-responsive thumbnail" src="@Url.Content(Model.Img2)" alt="Image" />
                    </div>

                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                        <img class="img-responsive thumbnail" src="@Url.Content(Model.Img3)" alt="Image" />
                    </div>

                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                        <img class="img-responsive thumbnail" src="@Url.Content(Model.Img4)" alt="Image" />
                    </div>

                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                        <img class="img-responsive thumbnail" src="@Url.Content(Model.Img5)" alt="Image" />
                    </div>

                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                        <img class="img-responsive thumbnail" src="@Url.Content(Model.Img6)" alt="Image" />
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section id="Life" class="container cont2">
        <div class="row">
            <div class="col-lg-3 col-md-3 col-sm-3"></div>
            <div class="col-lg-9 col-md-9 col-sm-9 corNav2">
                <span class="tituloSessao corTitulo">@Html.DisplayFor(model => model.TituloSessao2)</span>

                <div class="row ">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12"> <br><br></div>
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <span class="corTexto textoSessao">
                            @Html.DisplayFor(model => model.TextoSessao2)
                        </span>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section id="Living" class="container cont2">
        <div class="row">
            <div class="col-lg-3 col-md-3 col-sm-3"></div>
            <div class="col-lg-9 col-md-9 col-sm-9 corNav2">
                <span class="tituloSessao corTitulo">@Html.DisplayFor(model => model.TituloSessao3).</span>

                <div class="row ">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <br><br>
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <span class="corTexto textoSessao">
                            @Html.DisplayFor(model => model.TextoSessao3)
                        </span>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section id="Reflection" class="container">
        <div class="row">
            <div class="col-lg-3 col-md-3 col-sm-3"></div>
            <div class="col-lg-9 col-md-9 col-sm-9 corNav2">
                <span class="tituloSessao corTitulo ">@Html.DisplayFor(model => model.TituloSessao4).</span>

                <div class="row ">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12"> <br><br></div>
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <span class="corTexto textoSessao">
                            @Html.DisplayFor(model => model.TextoSessao4)
                        </span>
                    </div>
                </div>

                <div class="row container-fluid text-center tamanhoTexto">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12"> <br><br> </div>

                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 thumbnail">
                        <img class="img-responsive" src="@Url.Content(Model.Img7)" alt="Image" />
                        <span class="textoSessao">@Html.DisplayFor(model => model.LegendaImg1)</span>
                    </div>

                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 thumbnail">
                        <img class="img-responsive" src="@Url.Content(Model.Img8)" alt="Image" />
                        <span class="textoSessao">@Html.DisplayFor(model => model.LegendaImg2)</span>
                    </div>

                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 thumbnail">
                        <img class="img-responsive" src="@Url.Content(Model.Img9)" alt="Image" />
                        <span class="textoSessao">@Html.DisplayFor(model => model.LegendaImg3)</span>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="container text-right">
        <div class="row jumbotron">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <span class="textoSessao ">GeraSite - Todos os direitos reservados</span>
            </div>
        </div>

    </section>
</body>
</html> 